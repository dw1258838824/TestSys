<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.system.register.mapper.TExamRegisterMapper">
    
    <resultMap type="TExamRegister" id="TExamRegisterResult">
        <result property="examRegisterId"    column="exam_register_id"    />
        <result property="studentId"    column="student_id"    />
        <result property="examId"    column="exam_id"    />
        <result property="state"    column="state"    />
        <result property="remark"    column="remark"    />
        <result property="examCode"    column="exam_code"    />
        <result property="ctime"    column="ctime"    />
        <result property="uid"    column="uid"    />
        <result property="liveurl"    column="liveurl"    />
        <result property="roomId"    column="room_id"    />
        
        
        
        <result property="params.examTitle"    column="exam_title"    />
        <result property="params.beginTime"    column="begin_time"    />
        <result property="params.endTime"    column="end_time"    />
        <result property="params.studentName"    column="student_name"    />
        <result property="params.idcard"    column="idcard"    />
        <result property="params.mobile"    column="mobile"    />
        <result property="params.userId"    column="user_id"    />
        <result property="params.judgeState"    column="judge_state"    />
        <result property="params.operateBeginTime"    column="operate_begin_time"    />
        <result property="params.operateEndTime"    column="operate_end_time"    />
        <result property="params.argueBeginTime"    column="argue_begin_time"    />
        <result property="params.argueEndTime"    column="argue_end_time"    />
        <result property="params.exState"    column="ex_state"    />
        <result property="params.levelName"    column="level_name"    />
        <result property="params.resId"    column="res_id"    />
        <result property="params.score"    column="score"    />
        
    </resultMap>

    <sql id="selectTExamRegisterVo">
        SELECT
			er.exam_register_id,
			er.student_id,
			er.exam_id,
			er.state,
			er.remark,
			er.exam_code,
			er.ctime,
			e.exam_title,
			e.begin_time,
			e.end_time,
			e.operate_begin_time,
			e.operate_end_time,
			e.argue_begin_time,
			e.argue_end_time,
			e.state ex_state,
			l.level_name,
			s.student_name,
			s.idcard,
			s.mobile,
			s.user_id,
			res.judge_state,
			er.uid,
			er.liveurl,
			res.id res_id,
			er.room_id,
			res.score
		FROM
			t_exam_register er
		LEFT JOIN t_exam e ON er.exam_id = e.exam_id
		LEFT JOIN t_student s ON s.student_id = er.student_id
		LEFT JOIN t_room_exam_student res ON res.exam_id = er.exam_id AND res.student_id = s.student_id
		LEFT JOIN t_level l ON e.level_id = l.level_id
    </sql>

    <select id="selectTExamRegisterList" parameterType="TExamRegister" resultMap="TExamRegisterResult">
        <include refid="selectTExamRegisterVo"/>
        <where>  
            <if test="studentId != null "> and er.student_id = #{studentId}</if>
            <if test="params.studentName != null and params.studentName != '' "> and s.student_name LIKE CONCAT('%',#{params.studentName},'%')</if>
            <if test="examId != null "> and er.exam_id = #{examId}</if>
            <if test="params.examTitle != null and params.examTitle !='' "> and e.exam_title = #{params.examTitle}</if>
            <if test="state != null  and state != ''"> and er.state = #{state}</if>
            <if test="examCode != null  and examCode != ''"> and er.exam_code = #{examCode}</if>
            <if test="ctime != null "> and er.ctime = #{ctime}</if>
             <if test="params.beginCtime != null and params.beginCtime != '' "> and er.ctime >= #{params.beginCtime}</if>
            <if test="params.endCtime != null and params.endCtime != '' "> and er.ctime <![CDATA[ <= ]]>  CONCAT(#{params.endCtime},' 23:59:59')</if>
            <if test="uid != null  and uid != ''"> and er.uid = #{uid}</if>
            <if test=" roomId != null and roomId != '' "> and er.room_id = #{roomId}</if>
            <if test=" params.isYesterday != null and params.isYesterday != '' "> and e.begin_time LIKE CONCAT( DATE_ADD( CURDATE(), INTERVAL 1 DAY ), '%' ) </if>
        </where>
		GROUP BY er.exam_register_id
        ORDER BY res.judge_state ASC,e.begin_time ASC
    </select>
    
    <select id="selectTExamRegisterById" parameterType="Long" resultMap="TExamRegisterResult">
        <include refid="selectTExamRegisterVo"/>
        where exam_register_id = #{examRegisterId}
    </select>
        
    <insert id="insertTExamRegister" parameterType="TExamRegister">
        insert into t_exam_register
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="examRegisterId != null ">exam_register_id,</if>
            <if test="studentId != null ">student_id,</if>
            <if test="examId != null ">exam_id,</if>
            <if test="state != null  and state != ''">state,</if>
            <if test="remark != null  and remark != ''">remark,</if>
            <if test="examCode != null  and examCode != ''">exam_code,</if>
            <if test="ctime != null ">ctime,</if>
            <if test="uid != null  and uid != ''">uid,</if>
            <if test="liveurl != null  and liveurl != ''">liveurl,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="examRegisterId != null ">#{examRegisterId},</if>
            <if test="studentId != null ">#{studentId},</if>
            <if test="examId != null ">#{examId},</if>
            <if test="state != null  and state != ''">#{state},</if>
            <if test="remark != null  and remark != ''">#{remark},</if>
            <if test="examCode != null  and examCode != ''">#{examCode},</if>
            <if test="ctime != null ">#{ctime},</if>
            <if test="uid != null  and uid != ''">#{uid},</if>
            <if test="liveurl != null  and liveurl != ''">#{liveurl},</if>
         </trim>
    </insert>

    <update id="updateTExamRegister" parameterType="TExamRegister">
        update t_exam_register
        <trim prefix="SET" suffixOverrides=",">
            <if test="studentId != null ">student_id = #{studentId},</if>
            <if test="examId != null ">exam_id = #{examId},</if>
            <if test="state != null  and state != ''">state = #{state},</if>
            <if test="remark != null  and remark != ''">remark = #{remark},</if>
            <if test="examCode != null  and examCode != ''">exam_code = #{examCode},</if>
            <if test="uid != null  and uid != ''">uid = #{uid},</if>
            <if test="liveurl != null  and liveurl != ''">liveurl = #{liveurl},</if>
            <if test="ctime != null ">ctime = #{ctime},</if>
        </trim>
        where exam_register_id = #{examRegisterId}
    </update>

    <delete id="deleteTExamRegisterById" parameterType="Long">
        delete from t_exam_register where exam_register_id = #{examRegisterId}
    </delete>

    <delete id="deleteTExamRegisterByIds" parameterType="String">
        delete from t_exam_register where exam_register_id in 
        <foreach item="examRegisterId" collection="array" open="(" separator="," close=")">
            #{examRegisterId}
        </foreach>
    </delete>
    
    <select id="selectTExamRegisterByStudentExam" parameterType="TExamRegister" resultMap="TExamRegisterResult">
        select 
			exam_register_id,
			student_id,
			exam_id,
			state,
			remark,
			exam_code,
			ctime
		from t_exam_register 
        where student_id = #{studentId} and exam_id = #{examId}
    </select>
</mapper>