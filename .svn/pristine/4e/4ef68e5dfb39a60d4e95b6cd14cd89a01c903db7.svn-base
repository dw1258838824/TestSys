<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.system.register.mapper.TExamRegisterMapper">
    
    <resultMap type="TExamRegister" id="TExamRegisterResult">
        <result property="examRegisterId"    column="exam_register_id"    />
        <result property="studentId"    column="student_id"    />
        <result property="examId"    column="exam_id"    />
        <result property="state"    column="state"    />
        <result property="remark"    column="remark"    />
        <result property="examCode"    column="exam_code"    />
        <result property="ctime"    column="ctime"    />
        
        
        
        <result property="params.examTitle"    column="exam_title"    />
        <result property="params.beginTime"    column="begin_time"    />
        <result property="params.endTime"    column="end_time"    />
        <result property="params.studentName"    column="student_name"    />
        <result property="params.idcard"    column="idcard"    />
        <result property="params.mobile"    column="mobile"    />
        <result property="params.userId"    column="user_id"    />
    </resultMap>

    <sql id="selectTExamRegisterVo">
        SELECT
			exam_register_id,
			er.student_id,
			er.exam_id,
			er.state,
			remark,
			exam_code,
			er.ctime,
			e.exam_title,
			e.begin_time,
			e.end_time,
			s.student_name,
			s.idcard,
			s.mobile,
			s.user_id
		FROM
			t_exam_register er
		LEFT JOIN t_exam e ON er.exam_id = e.exam_id
		LEFT JOIN t_student s ON s.student_id = er.student_id
    </sql>

    <select id="selectTExamRegisterList" parameterType="TExamRegister" resultMap="TExamRegisterResult">
        <include refid="selectTExamRegisterVo"/>
        <where>  
            <if test="studentId != null "> and er.student_id = #{studentId}</if>
            <if test="examId != null "> and er.exam_id = #{examId}</if>
            <if test="state != null  and state != ''"> and er.state = #{state}</if>
            <if test="examCode != null  and examCode != ''"> and exam_code = #{examCode}</if>
            <if test="ctime != null "> and er.ctime = #{ctime}</if>
        </where>
    </select>
    
    <select id="selectTExamRegisterById" parameterType="Long" resultMap="TExamRegisterResult">
        <include refid="selectTExamRegisterVo"/>
        where exam_register_id = #{examRegisterId}
    </select>
        
    <insert id="insertTExamRegister" parameterType="TExamRegister">
        insert into t_exam_register
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="examRegisterId != null ">exam_register_id,</if>
            <if test="studentId != null ">student_id,</if>
            <if test="examId != null ">exam_id,</if>
            <if test="state != null  and state != ''">state,</if>
            <if test="remark != null  and remark != ''">remark,</if>
            <if test="examCode != null  and examCode != ''">exam_code,</if>
            <if test="ctime != null ">ctime,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="examRegisterId != null ">#{examRegisterId},</if>
            <if test="studentId != null ">#{studentId},</if>
            <if test="examId != null ">#{examId},</if>
            <if test="state != null  and state != ''">#{state},</if>
            <if test="remark != null  and remark != ''">#{remark},</if>
            <if test="examCode != null  and examCode != ''">#{examCode},</if>
            <if test="ctime != null ">#{ctime},</if>
         </trim>
    </insert>

    <update id="updateTExamRegister" parameterType="TExamRegister">
        update t_exam_register
        <trim prefix="SET" suffixOverrides=",">
            <if test="studentId != null ">student_id = #{studentId},</if>
            <if test="examId != null ">exam_id = #{examId},</if>
            <if test="state != null  and state != ''">state = #{state},</if>
            <if test="remark != null  and remark != ''">remark = #{remark},</if>
            <if test="examCode != null  and examCode != ''">exam_code = #{examCode},</if>
            <if test="ctime != null ">ctime = #{ctime},</if>
        </trim>
        where exam_register_id = #{examRegisterId}
    </update>

    <delete id="deleteTExamRegisterById" parameterType="Long">
        delete from t_exam_register where exam_register_id = #{examRegisterId}
    </delete>

    <delete id="deleteTExamRegisterByIds" parameterType="String">
        delete from t_exam_register where exam_register_id in 
        <foreach item="examRegisterId" collection="array" open="(" separator="," close=")">
            #{examRegisterId}
        </foreach>
    </delete>
    
</mapper>