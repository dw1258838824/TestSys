<body class="signin" >
	<img id="logo" th:src="@{/img/system.png}"  />
	<div id="lineBlue" style="float: right;margin: 20px;">
		<a id="reg" th:href="@{/login?flag=root}" style="float:right">后台登录 </a>
		<a id="reg2" th:href="@{/login}" style="float:right">学生登录 | &nbsp; </a>
	</div>
	<div class="menu" >
		<ul>
			<li id="index" onclick="location.href='login'">首页</li>
			<li onclick="company()">授权单位</li>
			<li onclick="location.href='login'">新闻资讯</li>
			<li onclick="cooperate()">商务洽谈</li>
			<li onclick="callMe()">联系我们</li>
		</ul>
	</div>
    <div id="sign" class="signinpanel">
        <div class="row centerDiv" style="height:250px">
            <div class="col-sm-5" id="notice">
                <div class="signin-info" id="notice">
                </div>
            </div>
            <div class="col-sm-3 loginDiv">
                <form id="signupForm" class="loginForm" autocomplete="off" >
                	<input type="hidden" name="flag" th:value="*{flag}" />
                    <h4 class="flagPlan"><font id="roleflag" color="black"></font></h4>
                    <!-- <p class="m-t-md">你若不离不弃，我必生死相依</p> -->
                    <input type="text"  id="username"   name="username" class="form-control uname"     placeholder="手机号" value=""    />
                    <input type="password" name="password" class="form-control pword"     placeholder="密码"   value="" />
					<div class="row m-t" th:if="${captchaEnabled==true}">
						<div class="col-xs-5">
						    <input type="text" name="validateCode" class="form-control code" placeholder="验证码" maxlength="5" />
						</div>
						<div class="col-xs-7">
							<a href="javascript:void(0);" title="点击更换验证码">
								<img th:src="@{captcha/captchaImage(type=${captchaType})}" class="imgcode" width="100%"/>
							</a>
						</div>
					</div>
                    <div class="checkbox-custom hidden" th:classappend="${captchaEnabled==false} ? 'm-t'">
				        <input type="checkbox" id="rememberme" name="rememberme"> <label for="rememberme">记住我</label>
				    </div>
                    <button class="btn btn-success btn-block" id="btnSubmit" data-loading="正在验证登录，请稍后...">登录</button>
                    <a id="reg3" th:href="@{/register}" style="float:right">学生注册</a>
                </form>
            </div>
        </div>
    </div>
    <div id="company" class="signinpanel" style="display:none">
       <div id="companyDate" class="row centerDiv">
            <div class="col-sm-3">
                <div class="ibox">
                    <div class="ibox-title">
                        <span class="label label-primary pull-right"></span>
                        <h5>	</h5>
                    </div>
                    <div class="ibox-content">
                        <font>简介:</font>
                        <p>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="cooperate" class="signinpanel" style="display:none">
       <div class="row centerDiv">
       	<div class="col-sm-12" style="height:300px"> 
                <form id="businessForm" class="signupForm" style="padding: 0px">
                    <div class="form-group">
                        <div class="col-sm-8">
                            <input name="startUserName" class="form-control" id="startUserName" aria-required="true" required type="text" placeholder="请输入姓名">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-8">
                            <input name="bsTel" class="form-control" id="bsTel" aria-required="true" required type="text" placeholder="请输入联系方式">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-8">
                            <textarea name="bsContext" class="form-control" id="bsContext" aria-required="true" required style="height:120px" placeholder="请输入洽谈内容"></textarea>
                            <button class="btn btn-success" type="button" id="business">提交</button>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-8 col-sm-offset-3">
                            
                        </div>
                    </div>
                </form>
            </div>
       </div>
    </div>
    <div id="callMe" class="signinpanel" style="display:none">
       <div class="row centerDiv">
       		<div class="col-sm-8">
                <div class="ibox">
                    <div class="ibox-content">
                        <p>联系电话：<font id="tel"></font>  </p>
                        <p>联系地址：<font id="address"></font>  </p>
                    </div>
                </div>
            </div>
       </div>
    </div>
    <div class="options" style="margin-top: 5.8%;padding: 0px;background-color:#eaedf4;height: 180px">
