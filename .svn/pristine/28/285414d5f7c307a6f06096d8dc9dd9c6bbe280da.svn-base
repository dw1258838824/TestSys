<script th:inline="javascript"> var ctx = [[@{/}]]; var captchaType = [[${captchaType}]]; </script>
<!-- 全局js -->
<script src="../static/js/jquery.min.js" th:src="@{/js/jquery.min.js}"></script>
<script src="../static/js/bootstrap.min.js" th:src="@{/js/bootstrap.min.js}"></script>
<!-- 验证插件 -->
<script src="../static/ajax/libs/validate/jquery.validate.min.js" th:src="@{/ajax/libs/validate/jquery.validate.min.js}"></script>
<script src="../static/ajax/libs/validate/messages_zh.min.js" th:src="@{/ajax/libs/validate/messages_zh.min.js}"></script>
<script src="../static/ajax/libs/layer/layer.min.js" th:src="@{/ajax/libs/layer/layer.min.js}"></script>
<script src="../static/ajax/libs/blockUI/jquery.blockUI.js" th:src="@{/ajax/libs/blockUI/jquery.blockUI.js}"></script>
<script src="../static/ruoyi/js/ry-ui.js" th:src="@{/ruoyi/js/ry-ui.js?v=4.2.0}"></script>
<script src="../static/ruoyi/login.js" th:src="@{/ruoyi/login.js}"></script>


 <script th:inline="javascript">
    	var notices = [[${notices}]];
    	var configs = [[${configs}]];
    	var callMeInfo = [[${callMeInfo}]];
    	var friendLines = [[${friendLines}]];
    	var flag = [[${flag}]];
    	
    	var times = setInterval(function(){
    		changeBg();
    	},5000);
    	var times2 = setInterval(function(){
    		changeNotice();
    	},7000);
    	
    	$(function(){
    		initHtml();
    		if(flag == "root"){
    			clearInterval(times);
    			clearInterval(times2);
        		$("#notice,.menu,#company,#cooperate,#callMe,.options,.project,.tearcher,.projectIng").hide();
    			$("#roleflag").html('后台登录');
    			$(".signup-footer").css("margin-top","14%");
    			$(".loginDiv").css("float","left");
    			$(".loginDiv").css("margin-left","35%");
    			$("#reg3").hide();
    		}else{
    			$("#roleflag").html('学生登录');
    			$("#index").attr("onclick","location.href='login'");
    			$("#username").attr("placeholder","手机号/身份证号");
    		}
    		$("#friendLine").html(friendLines);
    		var head = '<h4>公告新闻 &nbsp;  &nbsp;  &nbsp; ';
    		var tent = '';
    		//$("#signupForm").css("height",$("#signupForm"));
    		if(notices.length > 0){
    			var n = 0;
    			for(var i=0; i<notices.length ;i++){
    				if(notices[i].noticeType=='2'){
    					head += '<a href="javascript:showPanel('+n+')"><i class="fa fa-circle" aria-hidden="true"></i> &nbsp </a>';
    					
    					var disp = "none";
    					if(n==0){
    						disp = "block";
    					}
    					tent += '<div onclick="showNoticeInfo(this)" class="panel panel-default " style="display: '+disp+'"> '+
			                 '      <div class="panel-body"><H3> '+notices[i].noticeTitle+'</H3>'+
			                 '		<div class="panel-p">'+notices[i].noticeContent+'</div>'+
			                 '		<p style="float:right">'+notices[i].createTime+'</p>'+
			                 '      </div>'+
	                   		 '</div>';
    					n ++;
    				}
    			}
    			head += '</h4>';
	    		$("#notice").html(head + tent);
    		}
        	$("#business").click(function(){
        		$.post("api/business/addBusiness",$("#businessForm").serialize(),function(msg){
        			if(msg == 'success'){
        				alert("提交成功，请耐心等待");
        				location.href = location.href;
        			}else{
        				$.modal.alert("提交失败，请重试");
        			}
        		})
        	});
        	
        	$(".project li").mouseover(function(){
        		var img = "http://47.93.241.224:8080/files/projects/" + $(this).attr("lang");
        		$(".project ul").css("background-image","url('"+img+"')");
        	});
    	})
    	function checkInfo(ele){
    		if($(ele).next().is(":hidden")){
    			$(ele).next().show(300);
    		}else{
    			$(ele).next().hide(300);
    		}
    		
    	}
		var i = 1;
    	function changeBg(){
    		if(i==1){
    			$(".signin").css("background-image","url('img/2.jpg')");
    			i = 2;
    		}else if (i==2){
    			$(".signin").css("background-image","url('img/3.jpg')");
    			i = 3;
    		}else{
    			$(".signin").css("background-image","url('img/1.jpg')");
    			i = 1;
    		}
		}
    	function changeNotice(){
    		var a = $(".panel-default").length;
    		for(var s=0;s<a;s++){
    			if(!$($(".panel-default")[s]).is(":hidden")){
    				var hideIndex = s;
        			var showIndex = s + 1;
        			if(s == (a-1)){
        				showIndex = 0;
        				s = -1;
        			}
    				$($(".panel-default")[hideIndex]).hide();
        			$($(".panel-default")[showIndex]).fadeIn(1200);
    				return;
    			}
    		}
    	}
    	
    	function showPanel(idx){
    		clearInterval(times2);
    		$(".panel-default").hide();
    		$($(".panel-default")[idx]).fadeIn(1200);
    		times2 = setInterval(function(){
    			changeNotice();
        	},7000);
    	}
    	function company(){
    		$("#sign,#cooperate,#callMe").hide();
    		$("#company").show();
    		$.get("api/company/list",function(data){
    			var html = "";
    			if(data.length>0){
    				for(var i=0;i<data.length;i++){
	   					html += '<div class="col-sm-3">'+
		    		            '   <div class="ibox">'+
		    		            '       <div class="ibox-title">'+
		    		            '            <h5>'+data[i].cname+'</h5>'+
		    		             '       </div>'+
		    		             '       <div class="ibox-content">'+
		    		             '           <font>简介:</font>'+
		    		             '           <p>'+data[i].synopsis+'</p>'+
		    		             '       </div>'+
		    		             '   </div>'+
		    		           ' </div>';
    				}
    			}
	    		$("#companyDate").html(html);
    		});
    	}
    	function cooperate(){
    		$("#sign,#company,#callMe").hide();
    		$("#cooperate").show();
    	}
    	function callMe(){
    		$("#sign,#cooperate,#company").hide();
    		$("#callMe").show();
    		$("#tel").html(callMeInfo.split('-')[0]);
    		$("#address").html(callMeInfo.split('-')[1]);
    	}
    	function initHtml(){
    		$("#projectBlock").html([[${projectBlock}]]);
    		$("#projectBlockText").html([[${projectBlockText}]]);
    		$("#projectBlockName1").html([[${projectBlockName1}]]);
    		$("#projectBlockName2").html([[${projectBlockName2}]]);
    		$("#projectBlockName3").html([[${projectBlockName3}]]);
    		$("#projectBlockName4").html([[${projectBlockName4}]]);
    		$("#teacherAuth").html([[${teacherAuth}]]);
    		$("#teacherAuthText").html([[${teacherAuthText}]]);
    		$("#teacherAuthTitle1").html([[${teacherAuthTitle1}]]);
    		$("#teacherAuthTitle2").html([[${teacherAuthTitle2}]]);
    		$("#teacherAuthTitle3").html([[${teacherAuthTitle3}]]);
    		$("#teacherAuthTitle4").html([[${teacherAuthTitle4}]]);
    		$("#teacherAuthTitle5").html([[${teacherAuthTitle5}]]);
    		$("#teacherAuthTitle6").html([[${teacherAuthTitle6}]]);
    		$("#teacherAuthInfo1").html([[${teacherAuthInfo1}]]);
    		$("#teacherAuthInfo2").html([[${teacherAuthInfo2}]]);
    		$("#teacherAuthInfo3").html([[${teacherAuthInfo3}]]);
    		$("#teacherAuthInfo4").html([[${teacherAuthInfo4}]]);
    		$("#teacherAuthInfo5").html([[${teacherAuthInfo5}]]);
    		$("#teacherAuthInfo6").html([[${teacherAuthInfo6}]]);
    		$("#projectIng").html([[${projectIng}]]);
    		$("#projectIngTitle1").html([[${projectIngTitle1}]]);
    		$("#projectIngTitle2").html([[${projectIngTitle2}]]);
    		$("#projectIngTitle3").html([[${projectIngTitle3}]]);
    		$("#projectIngText1").html([[${projectIngText1}]]);
    		$("#projectIngText2").html([[${projectIngText2}]]);
    		$("#projectIngText3").html([[${projectIngText3}]]);
    		
    	}
    	function showNoticeInfo(ele){
    		$.modal.showInfo("查看详情",$(ele).html().replace('panel-p','panel-p2'),"80%","500px");
    	}
    </script>
</body>
