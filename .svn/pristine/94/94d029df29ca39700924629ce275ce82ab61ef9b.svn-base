 .Loginpanel {
  width: 400px;
  text-align:center;
  margin:auto;
  margin-bottom:50px;
  background: rgba(60, 65, 75, 0.6);
  -webkit-box-shadow: 0px 43px 83px -20px rgba(0, 0, 0, 0.41);
  -moz-box-shadow: 0px 43px 83px -20px rgba(0, 0, 0, 0.41);
  -ms-box-shadow: 0px 43px 83px -20px rgba(0, 0, 0, 0.41);
  -o-box-shadow: 0px 43px 83px -20px rgba(0, 0, 0, 0.41);
  box-shadow: 0px 23px 43px -20px rgba(0, 0, 0, 0.41);
  border-radius:35px;
}

.Loginpanel input[type="text"],.Loginpanel input[type="password"]{
  background: rgba(0, 0, 0, 0);
  border:0;
  border-bottom:2px solid #c8c9ca;
  box-shadow: none;
  font-weight: 300;
  font-size: 18px;
  padding: 5px 10px !important;
  padding-left:60px !important;
  margin:5px auto;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  -ms-border-radius: 0;
  border-radius: 0;
  color: #999 !important;
}
.Loginpanel input[type="text"]::-webkit-input-placeholder {
  /* Chrome/Opera/Safari */
  color: #999 !important;
}
.Loginpanel input[type="text"]::-moz-placeholder {
  /* Firefox 19+ */
  color: #999 !important;
}
.Loginpanel input[type="text"]:-ms-input-placeholder {
  /* IE 10+ */
  color: #999 !important;
}
.Loginpanel input[type="text"]:-moz-placeholder {
  /* Firefox 18- */
  color: #999 !important;
}

.Loginpanel input[type="password"]::-webkit-input-placeholder {
  /* Chrome/Opera/Safari */
  color: #999 !important;
}
.Loginpanel input[type="password"]::-moz-placeholder {
  /* Firefox 19+ */
  color: #999 !important;
}
.Loginpanel input[type="password"]:-ms-input-placeholder {
  /* IE 10+ */
  color: #999 !important;
}
.Loginpanel input[type="password"]:-moz-placeholder {
  /* Firefox 18- */
  color: #999 !important;
}

.Loginpanel-content .form-control {
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  -ms-box-shadow: none;
  -o-box-shadow: none;
  box-shadow: none;
  border: none;
  border: 2px solid rgba(0, 0, 0, 0.1);
  -webkit-border-radius: 0px;
  -moz-border-radius: 0px;
  -ms-border-radius: 0px;
  border-radius: 0px;
}

.Loginpanel-content .form-control:focus, .Loginpanel-content .form-control:active {
  border-bottom: 2px solid rgba(0, 160,230, 1);
}


.Loginpanel-title{
	width:400px;
	height:78px;
	background:rgba(200, 200, 200, 0.1);
	border-radius:35px 35px 0 0;
}
.Loginpanel-title div{
	width:100%;
	height:78px;
	padding:5px 38px;
	border:0px solid red;
}
.Loginpanel-title span,.Loginpanel-title h1{
	float:left;
	height:68px;
	line-height:68px;

}
.Loginpanel-title span{
	font-size:33px;
	margin-right:20px;
	padding-top:3px;
	color:#eeeeee !important;
}
.Loginpanel-title h1{
	font-size:28px;
	color:#eeeeee !important;
}
.Loginpanel-content {
  	padding:30px 15px;
	width:400px;
	height:320px;
	background:rgba(60,65,75,0.7);
	border-radius:0 0 35px 35px;
}

.Loginpanel-content .btn {
  letter-spacing: 100;
  text-transform: none;
  font-size: 18px;
  border-radius:10px;
  margin:10px auto;
}


.Loginpanel-content #mobile input{
  background:url(../images/user-bg.png) center left no-repeat;
}

.Loginpanel-content #password input{
  background:url(../images/pass-bg.png) center left no-repeat;
}

.Loginpanel-content #yzm{
  display:inline-block;
  padding:0;
}

.Loginpanel-content #yzm #code input{
  display:inline-block;
  width:180px;
  height:40px; 
  font-size:14px;
  background:#ddd;
  padding:0;
  padding-left:10px !important;
  color:red !important;
  border-radius:5px;
  border:1px solid #fff;
  margin:0px;
}

.Loginpanel-content #sbtn{
 margin:auto;
}

.Loginpanel-content #sbtn input{
 margin:0 !important;
 height:40px;
 line-height:40px;
 padding:0;
}

.Loginpanel-content #num img {
  height:40px;
  margin:0;
  border-radius:5px;
}

