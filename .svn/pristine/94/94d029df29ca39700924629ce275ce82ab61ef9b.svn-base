body {
  top:0;
  bottom:0;
  margin:0;
  padding:0;
  font-family: 'PingFang SC',Arial,'Microsoft YaHei',sans-serif;;
  background: #000;
  font-weight: 300;
  overflow-x: auto;
}

p, ul, ol {
  margin-bottom: 1.5em;
  color: #848484;
}

h1, h2, h3, h4, h5, h6 {
  color: #5a5a5a;
  font-weight: 400;
  margin: 0 0 20px 0;
}


#fh5co-header-section {
  padding: 0;
  position: relative;
  left: 0;
  margin:0;
  width: 100%;
  z-index: 1022;
}


.login-logo{
	margin-top: 35px; 
	margin-bottom:30px;
	max-height:80px;
	text-align:center;
	overflow:hidden;
}

.login-logo img{
	max-height:70px;
}

.container {
  width:1170px !important;
  margin:auto;
  border:0px solid red;
}

.fh5co-login {
  width:100%;
  top:50%;
  position: relative;
  overflow:hidden;
  border:0px solid red;
}

.mt {
	width:100%;
	height:48px;
	line-height:48px;
	padding:0 15px;
	overflow:hidden;
	margin-bottom:20px;
	border:0px solid red;
}


